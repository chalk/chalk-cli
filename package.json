{
	"name": "chalk-cli",
	"version": "4.1.0",
	"description": "Terminal string styling done right",
	"license": "MIT",
	"repository": "chalk/chalk-cli",
	"author": {
		"name": "Sindre Sorhus",
		"email": "sindresorhus@gmail.com",
		"url": "https://sindresorhus.com"
	},
	"bin": {
		"chalk": "cli.js"
	},
	"type": "module",
	"engines": {
		"node": "^12.20.0 || ^14.13.1 || >=16.0.0"
	},
	"xo": {
		"rules": {
			"comma-dangle": ["error", "only-multiline"],
			"promise/prefer-await-to-then": "error",
			"arrow-body-style": "error"
		}
	},
	"scripts": {
		"test": "xo && FORCE_COLOR=1 ava"
	},
	"files": [
		"cli.js"
	],
	"keywords": [
		"cli-app",
		"cli",
		"color",
		"colour",
		"colors",
		"terminal",
		"console",
		"string",
		"str",
		"ansi",
		"style",
		"styles",
		"tty",
		"formatting",
		"rgb",
		"256",
		"shell",
		"xterm",
		"log",
		"logging",
		"command-line",
		"text",
		"template",
		"tpl"
	],
	"dependencies": {
		"ansi-styles": "^3.1.0",
		"chalk": "^2.0.0",
		"dot-prop": "^3.0.0",
		"get-stdin": "^6.0.0",
		"meow": "^9.0.0"
	},
	"devDependencies": {
		"ava": "^0.25.0",
		"execa": "^1.0.0",
		"xo": "^0.44.0"
	}
}
